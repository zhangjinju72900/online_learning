<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!--tInformation  model -->
		<object name="tGoods" table="t_goods" primary="id"
			unique="" order="">
			<property name="id" type="Long" length="20" initial=""
				field="id" />
			<property name="name" type="String" length="500" initial=""
				field="name" />
			<property name="videoId" type="Long" length="20" initial=""
				field="video_id" />
			<property name="collectCount" type="Long" length="20"
				initial="" field="collect_count" />
			<property name="saleStatus" type="Long" length="20"
				initial="" field="sale_status" />
			<property name="validFlag" type="Long" length="10" initial=""
				field="valid_flag" />
			<property name="createTime" type="Date" length="" validate=""
				field="create_time" />
			<property name="createBy" type="Long" length="10" validate=""
				field="create_by" />
			<property name="updateTime" type="Date" length="" validate=""
				field="update_time" />
			<property name="updateBy" type="Long" length="10" validate=""
				field="update_by" />

			<!-- 新增 -->
			<property name="createByName" type="String" length="50"
				initial="" field="" />
			<property name="updateByName" type="String" length="50"
				initial="" field="" /> 
			<property name="collectBy" type="String" length="50"
				initial="" field="" /> 
		</object>
		
	</model_layer>
</tsoftware>
