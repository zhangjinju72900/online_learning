<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
   xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
  <object name="section" table="t_section" primary="id" unique="" order=""> 
   <property name="id" type="Long" length="20" initial="" field="id"/>
   <property name="name" type="String" length="50" initial="" field="name"/>
   <property name="remark" type="String" length="500" initial="" field="remark"/>
   <property name="courseId" type="Long" length="20" initial="" field="course_id"/>
   <property name="showOrder" type="Long" length="2" initial="" field="show_order"/>
   <property name="validFlag" type="Long" length="1" initial="" field="valid_flag"/>
   <property name="createTime" type="Date" length="20" initial="" field="create_time"/>
   <property name="createBy" type="Long" length="20" initial="" field="create_by"/>
   <property name="updateTime" type="Date" length="20" initial="" field="update_time"/>
   <property name="updateBy" type="Long" length="20" initial="" field="update_by"/>
  </object>
  
  
  <object name="QueryResourceP" table="t_course_resources_score" primary="id" unique="" order=""> 
   <property name="csrId" type="String" length="20" initial="" field="id"/>
   <property name="resourceId" type="String" length="20" initial="" field="customer_resources_id"/>
   <property name="courseId" type="String" length="20" initial="" field="course_id"/>
   <property name="sectionId" type="String" length="20" initial="" field="section_id"/>
   <property name="labelId" type="String" length="20" initial="" field="label_id"/>
   <property name="customerUserId" type="String" length="20" initial="" field="customer_user_id"/>
   <property name="token" type="String" length="500" initial="" field=""/>
   <property name="url" type="String" length="500" initial="" field=""/>
   <property name="module" type="String" length="500" initial="" field=""/>
   <property name="userId" type="Long" length="20" initial="" field=""/>
  </object>      
</model_layer>
</tsoftware>