		<property name="sectionId" type="Long" length="100" initial="" field="section_id"/>
		<property name="courseCount" type="Long" length="100" initial="" field="course_count"/>
		<property name="studentCount" type="Long" length="100" initial="" field="student_count"/>
		<property name="guideTeacherId" type="Long" length="100" initial="" field="guide_teacher_id"/>
		<property name="tname" type="Long" length="100" initial="" field=""/>
		<property name="teachingObject" type="String" length="100" initial="" field="teaching_object"/>
		<property name="teachingSite" type="String" length="100" initial="" field="teaching_site"/>
		<property name="equipment" type="String" length="200" initial="" field="equipment"/>
		<property name="customerTask" type="String" length="200" initial="" field="customer_task"/>
		<property name="teachingGoal" type="String" length="200" initial="" field="teaching_goal"/>
		<property name="teachingContent" type="String" length="200" initial="" field="teaching_content"/>
		<property name="homeworkContent" type="String" length="200" initial="" field="homework_content"/>
		<property name="teacherId" type="Long" length="100" initial="" field="teacher_id"/>
		<property name="validFlag" type="Long" length="1" initial="" field="valid_flag"/>
		
		<property name="createTime" type="Date" length="20" initial="" field="create_time"/>
		<property name="createBy" type="Long" length="20" initial="" field="create_by"/>
		<property name="updateTime" type="Date" length="20" initial="" field="update_time"/>
		<property name="updateBy" type="Long" length="20" initial="" field="update_by"/>
		<property name="schoolId" type="Long" length="20" initial="" field="school_id"/>
		<property name="createDate" type="Long" length="100" initial="" field=""/>
		
		<property name="className" type="String" length="500" initial="" field=""/>
