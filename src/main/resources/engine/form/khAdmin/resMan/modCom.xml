<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
	<!-- t_talking表原有字段 -->
	<object name="com" table="t_community" primary="id" unique="title" order="id desc" autoinc="Y">
		<property name="id" type="Long" length="20" initial="" field="id"/>
		<property name="name" type="String" length="500" initial="" field="name"/>
		<property name="parent" type="Long" length="20" initial="" field="parent_id"/>
		<property name="createComTime" type="Date" length="100" field="create_time"/>
		<property name="createComBy" type="String" length="50" field="create_by"/>
	</object>
</model_layer>
</tsoftware>
