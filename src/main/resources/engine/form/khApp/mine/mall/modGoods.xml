<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
   xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
  <object name="goods" table="t_goods" primary="id" unique="" order=""> 
   <property name="ids" type="Long" length="20" initial="" field="id"/>
   <property name="name" type="String" length="100" initial="" field="name"/>
   <property name="collectCount" type="Long" length="10" initial="" field="collect_count"/>
   <property name="videoId" type="Long" length="20" initial="" field="video_id"/>
   <property name="content" type="String" length="" initial="" field="content"/>
   <property name="saleStatus" type="Long" length="1" initial="" field="sale_status"/>
   <property name="validFlag" type="Long" length="1" initial="" field="valid_flag"/>
   <property name="createTime" type="Date" length="20" initial="" field="create_time"/>
   <property name="createBy" type="Long" length="20" initial="" field="create_by"/>
   <property name="updateTime" type="Date" length="20" initial="" field="update_time"/>
   <property name="updateBy" type="Long" length="20" initial="" field="update_by"/>
   <property name="integral" type="Long" length="10" initial="" field=""/>
   <property name="amount" type="Double" length="12" initial="" field=""/>
   <property name="picId" type="Long" length="20" initial="" field=""/>
   <property name="payId" type="Long" length="20" initial="" field=""/>

  </object> 
  
       <!-- 商品收藏 -->    
   <object name="goodsCollect" table="t_goods_collect" primary="id" unique="" order=""> 
   <property name="id" type="Long" length="20" initial="" field="id"/>
   <property name="goodId" type="Long" length="20" initial="" field="good_id"/>
   <property name="collectBy" type="Long" length="20" initial="" field="collect_by"/>
   <property name="collectTime" type="Date" length="20" initial="" field="collect_time"/>
   <property name="collectCount" type="Long" length="2" initial="" field="collect_count"/>
   <property name="createTime" type="Date" length="20" initial="" field="create_time"/>
   <property name="createBy" type="Long" length="20" initial="" field="create_by"/>
   <property name="updateTime" type="Date" length="20" initial="" field="update_time"/>
   <property name="updateBy" type="Long" length="20" initial="" field="update_by"/>
  </object>     

</model_layer>
</tsoftware>