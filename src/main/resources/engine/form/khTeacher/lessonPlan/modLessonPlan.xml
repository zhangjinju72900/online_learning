<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
	<object name="modLessonPlan" table="t_lesson_plan" primary="id" unique="" order="id desc" autoinc="Y">
		<property name="id" type="Long" length="200" initial="" field="id"/>
		<property name="name" type="String" length="200" initial="" field="name"/>
		<property name="classId" type="Long" length="100" initial="" field="class_id"/>
		<property name="lessonId" type="Long" length="100" initial="" field="course_id"/>
