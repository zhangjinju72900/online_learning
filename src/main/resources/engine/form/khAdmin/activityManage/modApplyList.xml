<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../../componentConfig.xsd">
    <model_layer>
        <object name="ApplyList" table="t_activity_apply" primary="id" unique="" order="id desc" autoinc="Y">
            <property name="id" type="Long" length="10" initial="" field="id"/>
            <property name="activityId" type="Long" length="20" initial="" field="activity_id"/>
            <property name="applyBy" type="Long" length="20" initial="" field="apply_by"/>
            <property name="applyTime" type="Date" length="" initial="" field="apply_time"/>
            <property name="applyType" type="Long" length="1" initial="" field="apply_type"/>
            <property name="validFlag" type="Long" length="1" initial="" field="valid_flag"/>

            <property name="updateTime" type="Date" length="" initial="" field="update_time"/>
            <property name="updateBy" type="Long" length="20" initial="" field="update_by"/>
            <property name="createTime" type="Date" length="" initial="" field="create_time"/>
            <property name="createBy" type="Long" length="20" initial="" field="create_by"/>

            <property name="name" type="String" length="20" initial="" field=""/>
            <property name="nickname" type="String" length="50" initial="" field=""/>
            <property name="sex" type="Long" length="1" initial="" field=""/>
            <property name="school" type="String" length="50" initial="" field=""/>
            <property name="class" type="String" length="50" initial="" field=""/>
            <property name="role" type="String" length="50" initial="" field=""/>
        </object>
    </model_layer>
</tsoftware>