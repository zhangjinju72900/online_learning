                <procedure name="procStartWorkflow" logic="StartWorkflow" ifyes="proTransition" ifno="">
                    <param name="Workflow" value="empEntryProcess"></param>
                    <param name="ViewUrl" value="frmEmpDetail"></param>
                    <param name="In" value="pnlDetailGroup.eq_empId"></param>
                </procedure>
                <procedure name="proTransition" server="/frmEmpList" logic="Back" ifyes="" ifno="">
                </procedure>

            </flow>
            <!--确认-->
            <flow trigger="pnlDetailTool.ctlConfirm" event="OnClick"
