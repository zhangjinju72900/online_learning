<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
	<object name="Banner" table="t_banner" primary="id" unique="" order="id desc" autoinc="Y">
	<property name="id" type="Long" length="10" initial="" field="id"/>
	<property name="filename" type="String" length="50" field=""/>
	<property name="fileId" type="Long" length="50" field="pic_id"/>
	<property name="aLink" type="String" length="300" field="alink"/>
	<property name="type" type="String" length="50" field="banner_type"/>
	<property name="typeName" type="String" length="50" field=""/>
	<property name="showorder" type="Long" length="50" field="show_order"/>
    <property name="createTime" type="Date" length="" validate=""  field="create_time" />
     <property name="createby" type="Long" length="50" field="create_by"/>
      <property name="createrName" type="Long" length="50" field=""/>
     <property name="updateTime" type="Date" length="" validate=""  field="update_time" />
     <property name="updateby" type="Long" length="50" field="update_by"/>
       <property name="updaterName" type="Long" length="50" field=""/>
	
  </object>       
</model_layer>
</tsoftware>
