<?xml version="1.0" encoding="UTF-8"?>
<tsoftware  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
			xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
<model_layer>
	<object name="modAttendance" table="t_attend_class_record" primary="id" unique="" order="id desc" autoinc="Y">
		<property name="id" type="String" length="200" initial="" field="id"/>
		<property name="classId" type="Long" length="20" initial="" field="class_id"/>
		<property name="schoolId" type="Long" length="20" initial="" field="school_id"/>
		<property name="courseName" type="String" length="200" initial="" field=""/>
		<property name="sectionName" type="String" length="500" initial="" field=""/>
		<property name="className" type="String" length="500" initial="" field=""/>
		<property name="startTime" type="String" length="500" initial="" field=""/>
		<property name="studentCount" type="String" length="500" initial="" field=""/>
		<property name="signRate" type="String" length="500" initial="" field=""/>
		<property name="createTime" type="String" length="500" initial="" field="create_time"/>
		
	</object>
	
	
	<object name="absenteeismInfo" table="t_customer_user" primary="id" unique="" order="id desc" autoinc="Y">
		<property name="studentId" type="String" length="200" initial="" field="id"/>
		<property name="name" type="String" length="200" initial="" field="name"/>
		<property name="title" type="String" length="500" initial="" field=""/>
	</object>
</model_layer>
</tsoftware>
