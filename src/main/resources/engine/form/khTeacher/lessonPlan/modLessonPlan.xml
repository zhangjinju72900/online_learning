		<property name="sectionName" type="String" length="200" initial="" field=""/>
		<property name="guideTeacherName" type="Long" length="100" initial="" field=""/>
		<property name="regionName" type="Long" length="100" initial="" field=""/>
	</object>
	
	<object name="modLessonStep" table="t_lesson_step" primary="id" unique="" order="id desc" autoinc="Y">
		<property name="id" type="Long" length="200" initial="" field="id"/>
		<property name="lessonPlanId" type="Long" length="200" initial="" field="lesson_plan_id"/>
		<!-- <property name="name" type="String" length="200" initial="" field="name"/> -->
  		<property name="bzname" type="String" length="200" initial="" field="name"/>
		<property name="teachingCount" type="Long" length="100" initial="" field="teaching_count"/>
		<!-- <property name="teachingContent" type="String" length="400" initial="" field="teaching_content"/> -->
  		<property name="steachingContent" type="String" length="400" initial="" field="teaching_content"/>
		<property name="teachingAssist" type="String" length="400" initial="" field="teaching_assist"/>
		<property name="teachingMethod" type="String" length="400" initial="" field="teaching_method"/>
		<property name="createTime" type="Date" length="20" initial="" field="create_time"/>
		<property name="createBy" type="Long" length="20" initial="" field="create_by"/>
		<property name="updateTime" type="Date" length="20" initial="" field="update_time"/>
		<property name="updateBy" type="Long" length="20" initial="" field="update_by"/>
	</object>
</model_layer>
</tsoftware>
